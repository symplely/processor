{
    "name": "uppes/processor",
    "description": "An Process control API for asynchronous PHP programming.",
    "keywords": [
        "process",
        "processor",
        "command",
        "task",
        "sub process",
        "async"
    ],
    "homepage": "https://github.com/uppes/processor",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lawrence Stubbs",
            "email": "lstubbs@techno.express"
        }
    ],
    "require": {
        "php": ">7.1",
        "uppes/eventloop": "^1.0.8",
        "opis/closure": "^3.0",
        "symfony/process": "^4.2"
    },
    "autoload": {
        "psr-4": { 
			"Async\\Processor\\": "Processor/"
		}
    },
    "require-dev": {
        "phpunit/phpunit": ">5.7"
    },
    "autoload-dev": {
        "psr-4": {
            "Async\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --bootstrap vendor/autoload.php tests"
    }
}
